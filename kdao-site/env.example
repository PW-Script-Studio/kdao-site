# KDAO 2.0 Environment Variables
# Copy this file to .env and fill in your values

# Private Keys (NEVER commit these!)
PRIVATE_KEY=your_private_key_here
DEPLOYER_PRIVATE_KEY=your_deployer_private_key_here

# RPC URLs
KASPLEX_TESTNET_RPC=https://testnet-rpc.kasplex.org
KASPLEX_MAINNET_RPC=https://rpc.kasplex.org
SEPOLIA_RPC=https://sepolia.infura.io/v3/your_infura_key
ETHEREUM_RPC=https://mainnet.infura.io/v3/your_infura_key

# API Keys for verification
KASPLEX_EXPLORER_API=your_kasplex_api_key
ETHERSCAN_API_KEY=your_etherscan_api_key
INFURA_KEY=your_infura_project_id
ALCHEMY_KEY=your_alchemy_key

# Gas Reporter
REPORT_GAS=true
COINMARKETCAP_API_KEY=your_coinmarketcap_api_key

# Contract Addresses (after deployment)
KDAO_TOKEN_ADDRESS=
GOVERNANCE_ADDRESS=
TREASURY_ADDRESS=
STAKING_ADDRESS=
ELECTION_ADDRESS=

# LP Token Address (KDAO/USDT on CoinEx)
LP_TOKEN_ADDRESS=

# Multisig Wallets
TREASURY_MULTISIG=
TEAM_MULTISIG=

# Network Chain IDs
KASPLEX_CHAIN_ID=12345
KASPLEX_TESTNET_CHAIN_ID=98765

# Frontend Configuration
NEXT_PUBLIC_NETWORK=kasplex_testnet
NEXT_PUBLIC_RPC_URL=https://testnet-rpc.kasplex.org
NEXT_PUBLIC_EXPLORER_URL=https://testnet.explorer.kasplex.org

# Discord Webhook (for notifications)
DISCORD_WEBHOOK_URL=

# IPFS (for proposal metadata)
IPFS_GATEWAY=https://ipfs.io/ipfs/
IPFS_API_URL=https://ipfs.infura.io:5001

# Debug Mode
DEBUG=false
VERBOSE_LOGGING=false